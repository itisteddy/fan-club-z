{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/media/search",
      "destination": "/api/media-search"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
