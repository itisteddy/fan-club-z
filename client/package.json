{
  "name": "@fanclubz/client",
  "version": "2.0.55",
  "description": "Fan Club Z React Client Application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "vercel-build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "generate-icons": "bash ../generate-pwa-icons.sh",
    "pwa-audit": "lighthouse --view --chrome-flags='--headless' --only-categories=pwa http://localhost:5173",
