{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://fan-club-z.onrender.com",
    "VITE_WS_URL": "wss://fan-club-z.onrender.com",
    "VITE_ENVIRONMENT": "production",
    "VITE_DEBUG": "false"
  }
}
